
//C++//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main()
{
	float a,b,c;
	cout<< "Escribir la variable a: ";
	cin>> a;
	cout<< "Escribir la variable b :";
	cin>>b;
	c=a;
	a=b;
	b=c;
	cout<< "Ahora a vale :"<<a<<endl;
	cout<< "Ahora b vale :"<<b<<endl;
	getch();
}

//Pseudocodigo//

Proceso CambiodeVariable
	Leer A
	Leer B
	aux<- A
	A<-B
	B<- aux
	Escribir "El valor de A es: ", A
	Escribir "El valor de B es: ", B
FinProceso



                               
